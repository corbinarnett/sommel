<%= form_for(@wine) do |f| %>
  <fieldset>
  <br>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>

  <%= f.label :grape %>
  <%= f.text_field :grape %>
  <br><br>

  <%= f.label :vintage %>
  <%= f.text_field :vintage %>
  <br><br>



  <%= f.label :producer_id %>
  <%= f.collection_select(:producer_id, Producer.all, :id, :name, include_blank: true) %>

    <%= f.submit "Add Wine" %>
  </fieldset>
<% end %>
  