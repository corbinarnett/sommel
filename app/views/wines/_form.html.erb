<%= form_for(@wine) do |f| %>
  <fieldset>
  <br>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>

  <%= f.label :vintage %>
  <%= f.text_field :vintage %>
  <br><br>

  <%= f.label :varietal %>
  <%= f.text_field :varietal %>
  <br><br>

  <%= f.label :winery_id %>
  <%= f.collection_select(:winery_id, Winery.all, :id, :name, include_blank: true) %>

    <%= f.submit "Add Wine" %>
  </fieldset>
<% end %>
  