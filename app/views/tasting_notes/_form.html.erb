<%= form_for @tasting_note do |f| %>
  <br><br>

   <%= f.label :wine_id %>
  <%= f.collection_select(:wine_id, Wine.all, :id, :name, include_blank: true) %>
  <br><br>


  <%= f.label :rating %>
  <%= f.number_field :rating, in: 1..5 %>
  
  <div>
  <%= f.label :comment %>
  <%= f.text_area :comment, size: "24x6" %>
  </div>

  <%= f.submit %>
<% end %>


