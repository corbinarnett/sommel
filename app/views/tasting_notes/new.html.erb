<%= form_for @tasting_note do |f| %>
  <br><br>

   <%= f.label :wine_id %>
  <%= f.collection_select(:wine_id, Wine.all, :id, :name, include_blank: true) %>
  <br><br>


  <%= f.label :rating %>
  <%= f.number_field :rating %>
  
  <div>
  <%= f.label :comment %>
  <%= f.text_area :comment, rows: 6 %>
  </div>

  <%= f.submit %>
<% end %>


