<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%# required:true does not protect on the backend %>
  <%= f.text_field :username   %>
  
  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

    <%=f.label :password_confirmation %>
    <%=f.password_field :password_confirmation%>
  <%= f.submit "Sign Up" %>
<% end %>