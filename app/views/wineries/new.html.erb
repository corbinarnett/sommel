<h1>Add Winery</h1>

<%= form_for(@winery) do |f| %>
  <fieldset>
  <br>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>

  <%= f.label :region_or_city %>
  <%= f.text_field :region_or_city %>
  <br><br>

  <%= f.label :country %>
  <%= f.text_field :country %>
  <br><br>

    <%= f.submit "Add Winery" %>
  </fieldset>
<% end %>